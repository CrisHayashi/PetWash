        <script src="/libs/sweetalert2/sweetalert2.min.js"></script>
        <!-- IMask -->
        <script src="/libs/imask/imask.min.js"></script>
        <!-- Chart.js -->
        <script src="/libs/chartjs/chart.min.js"></script>
        <!-- Scripts específicos da página -->
        <%- typeof scripts !=='undefined' ? scripts : '' %>
